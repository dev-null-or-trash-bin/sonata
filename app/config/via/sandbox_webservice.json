{
    "name": "Blackbox",
    "apiVersion": "1.0",
    "baseUrl": "http://sandbox.api.via.de",
    "operations": {
        "PostAuthentication": {
            "httpMethod": "POST",
            "uri": "Authentication_JSON_AppService.axd/Login",
            "responseClass": "Via\\Bundle\\GuzzleBundle\\Response\\ViaEbayAuthResponse",
            "parameters": {
                "userName": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "password": {
                    "location": "json",
                    "type": "string",
                    "required": true
                    
                },
                "createPersistentCookie": {
                    "location": "json",
                    "type": "string",
                    "default": "false"
                }
            }
        },
        "GetProducts": {
            "httpMethod": "GET",
            "uri": "publicapi/v1/api.svc/Products",
            "responseClass": "GetAuthOutput"            
        },
        "GetCatalogs": {
            "httpMethod": "GET",
            "uri": "publicapi/v1/api.svc/Catalogs",
            "responseClass": "GetAuthOutput"            
        }
    },
    "models": {
        "GetAuthOutput": {
            "type": "object",
            "additionalProperties": {
                "location": "json"
            }
        }
    }
}