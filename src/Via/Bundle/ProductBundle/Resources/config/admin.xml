<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    
    <parameters>        
        <parameter key="via.product.admin.product_property.class">Via\Bundle\ProductBundle\Admin\ProductPropertyAdmin</parameter>
        <parameter key="via.product.admin.product.class">Via\Bundle\ProductBundle\Admin\ProductAdmin</parameter>        
    </parameters>
    
    <services>
    
       <service id="via.product.admin.product" class="%via.product.admin.product.class%">
          <tag name="sonata.admin" manager_type="orm" 
                group="Content" 
                label="via.product.admin.dashboard.product"
           		label_translator_strategy="sonata.admin.label.strategy.noop"     
           />
          <argument />
          <argument>%via.product.entity.product.class%</argument>
          <argument>SonataAdminBundle:CRUD</argument>
          <argument type="service" id="service_container" />
          <call method="setTranslationDomain">
              <argument>messages</argument>
          </call>
      </service>
      
      <service id="via.product.admin.product_property" class="%via.product.admin.product_property.class%">
          <tag name="sonata.admin" manager_type="orm" 
                group="Content" 
                label="via.product.admin.dashboard.product_property"
           		label_translator_strategy="sonata.admin.label.strategy.noop"     
           />          
          <argument />
          <argument>%via.product.entity.product_property.class%</argument>
          <argument />
          <call method="setTranslationDomain">
              <argument>messages</argument>
          </call>
      </service>
      
   </services>
   
   
</container>