<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    
    <parameters>        
        <parameter key="via.admin.property.class">Via\Bundle\PropertyBundle\Admin\PropertyAdmin</parameter>
        <parameter key="via.entity.property.class">Via\Bundle\PropertyBundle\Entity\Property</parameter>        
    </parameters>
    
    <services>
       <service id="sonata.admin.properties" class="%via.admin.property.class%">
          <tag name="sonata.admin" manager_type="orm" 
                group="Content" 
                label="via.admin.dashboard.properties"/>
          <argument />
          <argument>%via.entity.property.class%</argument>
          <argument />
          <call method="setTranslationDomain">
              <argument>messages</argument>
          </call>
      </service>
   </services>
</container>